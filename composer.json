{
    "name": "richcongress/unit-test-bundle",
    "description": "This bundle gives a set of tools to test your application with Unit tests.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "RichCongress",
            "homepage": "https://www.richcongress.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "brianium/paratest": "^2.0|^3.0|^4.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "symfony/dependency-injection": "^3.0 || ^4.0 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "doctrine/annotations": "^1.11@dev",
        "doctrine/orm": "^2.0|^3.0",
        "php-coveralls/php-coveralls": "^2.1",
        "roave/security-advisories": "dev-master",
        "symfony/security": "^4.0|^5.0",
        "symfony/translation": "^4.0|^5.0",
        "symfony/validator": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "RichCongress\\UnitTestBundle\\": ""
        },
        "files": [
            "./Helper/GlobalNamespaceHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "RichCongress\\UnitTestBundle\\Tests\\": "Tests/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "discard-changes": true,
        "sort-packages": true
    }
}
